Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"activateHidden", "[", 
    RowBox[{
    "boxLength_", ",", "latt_", ",", "w_", ",", "b_", ",", "binary_"}], "]"}],
    ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"hiddenLatt", ",", "idxs", ",", "vals", ",", "p", ",", "act"}], 
      "}"}], "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"hiddenLatt", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", "boxLength"}], "]"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"i", "\[Equal]", "boxLength"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"idxs", "=", 
             RowBox[{"{", "i", "}"}]}], ";"}]}], "\[IndentingNewLine]", "]"}],
          ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"i", "\[NotEqual]", "boxLength"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"idxs", "=", 
             RowBox[{"{", 
              RowBox[{"i", ",", 
               RowBox[{"i", "+", "1"}]}], "}"}]}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"act", "=", "b"}], ";", "\[IndentingNewLine]", 
         RowBox[{"Do", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"act", "+=", 
             RowBox[{"w", "*", 
              RowBox[{"latt", "[", 
               RowBox[{"[", "idx", "]"}], "]"}]}]}], ";"}], 
           "\[IndentingNewLine]", ",", 
           RowBox[{"{", 
            RowBox[{"idx", ",", "idxs"}], "}"}]}], "]"}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"act", "=", 
          RowBox[{"LogisticSigmoid", "[", "act", "]"}]}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{"binary", "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"RandomReal", "[", 
                RowBox[{"{", 
                 RowBox[{"0", ",", "1"}], "}"}], "]"}], "<", "act"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"hiddenLatt", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "=", "1.0"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"hiddenLatt", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "=", "0.0"}]}], 
             "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"hiddenLatt", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "=", "act"}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "boxLength"}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "hiddenLatt", "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
